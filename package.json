{
  "name": "chat-server",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-chatServer",
    "stop": "egg-scripts stop --title=egg-server-chatServer",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --fix",
    "clean": "ets clean",
    "generator-entity": "node ./generator/index.js && npm run lint-fix && sequelize db:migrate"
  },
  "dependencies": {
    "await-stream-ready": "^1.0.1",
    "dayjs": "^1.10.7",
    "egg": "^2.6.1",
    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-mailer": "^1.5.0",
    "egg-passport": "^2.1.1",
    "egg-passport-github": "^1.0.0",
    "egg-scripts": "^2.6.0",
    "egg-sequelize": "^6.0.0",
    "egg-validate": "^2.0.2",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mysql2": "^2.3.0",
    "svg-captcha": "^1.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "sequelize-cli": "^6.2.0",
    "tslib": "^1.9.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
